<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#383A3A"
    tools:context="com.example.Application.user1">

    <Button
        android:id="@+id/LogOut"
        android:layout_width="wrap_content"
        android:layout_height="45dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="269dp"
        android:layout_marginTop="34dp"
        android:layout_marginEnd="9dp"
        android:layout_marginBottom="95dp"
        android:background="#92A2A2"
        android:text="logout"
        android:textColor="#090808"
        android:textSize="24sp" />

    <Button
        android:id="@+id/shoppingCart"
        android:layout_width="wrap_content"
        android:layout_height="99dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="92dp"
        android:layout_marginEnd="105dp"
        android:layout_marginBottom="165dp"
        android:background="#92A2A2"
        android:text="Shopping Cart"
        android:textColor="#090808"
        android:textSize="24sp" />

    <Button
        android:id="@+id/itemsList"
        android:layout_width="212dp"
        android:layout_height="90dp"
        android:layout_alignTop="@id/Welcome"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="88dp"
        android:layout_marginTop="158dp"
        android:layout_marginEnd="93dp"
        android:background="#92A2A2"
        android:text="Items List"
        android:textColor="#090808"
        android:textSize="24sp" />

    <TextView
        android:id="@+id/Welcome"
        android:layout_width="wrap_content"
        android:layout_height="42dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="72dp"
        android:layout_marginTop="134dp"
        android:layout_marginEnd="53dp"
        android:layout_marginBottom="609dp"
        android:text="Welcome to Home Page"
        android:textColor="#FCF8F8"
        android:textSize="24sp" />
</RelativeLayout>
